{
  "name": "cut-editor-electron",
  "version": "1.0.0",
  "description": "Cut Editor - Electron Migration",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently --kill-others --names \"MAIN,PRELOAD,RENDERER,ELECTRON\" --prefix-colors \"yellow,cyan,green,blue\" \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:main": "webpack --config webpack.main.config.js --mode development --watch",
    "dev:preload": "webpack --config webpack.preload.config.js --mode development --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js --mode development",
    "dev:electron": "wait-on dist/main/index.js dist/preload/index.js http://localhost:3000 && nodemon --exec electron . --watch dist/main --watch dist/preload --ext js",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:preload": "webpack --config webpack.preload.config.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "start": "electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "check-all": "npm run lint && npm run typecheck && npm run format",
    "pre-commit": "npm run check-all"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "image-editor"
  ],
  "author": "sooyeol",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/fabric": "^5.3.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.0",
    "electron": "^25.0.0",
    
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.0",
    "postcss-loader": "^7.3.0",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "wait-on": "^8.0.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "fabric": "^5.3.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.0"
  }
}